<div class="container">
  <% @locations.each do |location| %>
      <div class="column">
        <div class="qr-container">
          <a href="<%= location.url %>" class="qr">
            <h1>QRTC.ART</h1>
            <div class="qr-code">
              <%= location.qr_svg %>
            </div>
          </a>
        </div>
      </div>
    </div>
  <% end %>
</div>
